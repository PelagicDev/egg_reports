<h1>Listing daily_reports</h1>

<table>
  <thead>
    <tr>
      <th>Hatch</th>
      <th>Cull</th>
      <th>Hen deaths</th>
      <th>Male deaths</th>
      <th>Hen feed</th>
      <th>Male feed</th>
      <th>House</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daily_reports.each do |daily_report| %>
      <tr>
        <td><%= daily_report.hatch_eggs %></td>
        <td><%= daily_report.cull_eggs %></td>
        <td><%= daily_report.hen_deaths %></td>
        <td><%= daily_report.male_deaths %></td>
        <td><%= daily_report.hen_feed %></td>
        <td><%= daily_report.male_feed %></td>
        <td><%= daily_report.house_id %></td>
        <td><%= link_to 'Show', daily_report %></td>
        <td><%= link_to 'Edit', edit_daily_report_path(daily_report) %></td>
        <td><%= link_to 'Destroy', daily_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Daily report', new_daily_report_path, class: 'btn btn-primary btn-lg' %>
